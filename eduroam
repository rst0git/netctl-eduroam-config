Description='Eduroam'
Interface=wlan0
Connection=wireless
Security=wpa-configsection
IP=dhcp
TimeoutWPA=10
WPAConfigSection=(
	'ssid="eduroam"'
	'scan_ssid=0'
	'priority=0'
	'mode=0'
	'proto=RSN'
	'key_mgmt=WPA-EAP'
	'auth_alg=OPEN'
	'pairwise=CCMP'
	'group=CCMP'
	'eap=PEAP'
	'identity="user@example"'
	'anonymous_identity="user@example"'
	'ca_cert="/usr/share/ca-certificates/trust-source/mozilla.trust.crt"'
	'password=hash:<pwd-hash>'
	'phase1="peaplabel=0"'
	'phase2="auth=MSCHAPv2"'
)
